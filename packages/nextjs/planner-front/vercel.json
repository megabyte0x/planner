{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "SENTRY_DISABLED": "true",
    "DISABLE_SENTRY": "true",
    "VITE_SENTRY_DISABLED": "true"
  },
  "build": {
    "env": {
      "SENTRY_DISABLED": "true",
      "VITE_SENTRY_DISABLED": "true"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Sentry-Disabled",
          "value": "true"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}